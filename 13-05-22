//program to print sub arrayys in an array

#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)cin>>arr[i];
    for(int i=0;i<n;i++)                              ////this loop is to fix start
    {
    for(int j=i;j<n;j++){                             ////this loop is to fix end
        for(int k=i;k<=j;k++){                           //loop is to bring all the elements that are in between start and end
            cout<<arr[k]<<" ";
        }
        cout<<"\n";
    }
    }

    return 0;
}
